{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "$SUPABASE_ACCESS_TOKEN"
      ],
      "env": {
        "SUPABASE_URL": "$NEXT_PUBLIC_SUPABASE_URL",
        "SUPABASE_ANON_KEY": "$NEXT_PUBLIC_SUPABASE_ANON_KEY"
      }
    }
  }
}